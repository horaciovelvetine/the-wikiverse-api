{
	"printWidth": 120,
	"tabWidth": 4,
	"useTabs": true,
	"semi": true,
	"singleQuote": false,
	"quoteProps": "as-needed",
	"trailingComma": "none",
	"bracketSpacing": true,
	"bracketSameLine": false,
	"arrowParens": "avoid",
	"endOfLine": "lf",
	"plugins": ["prettier-plugin-java"],
	"overrides": [
		{
			"files": "*.java",
			"options": {
				"printWidth": 120,
				"tabWidth": 4,
				"useTabs": true,
				"trailingComma": "none"
			}
		}
	]
}
